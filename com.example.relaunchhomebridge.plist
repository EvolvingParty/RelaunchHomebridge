<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>KeepAlive</key>
<dict>
	<key>OtherJobEnabled</key>
	<dict>
		<key>com.homebridge.server</key>
		<false/>
	</dict>
</dict>
  <key>RunAtLoad</key>
    <true/>
        <key>Label</key>
        <string>com.example.relaunchhomebridge</string>
        <key>ProgramArguments</key>
        <array>
                <string>sh</string>
                <string>-c</string>
                <string>date; launchctl load /Library/LaunchDaemons/com.homebridge.server.plist</string>
        </array>
        <key>StandardOutPath</key>
            <string>/Users/USER/relaunchhomebridge output.log</string>
        <key>StandardErrorPath</key>
            <string>/Users/USER/relaunchhomebridge output.log</string>
        <key>StartInterval</key>
        <integer>7201</integer>
        <key>ThrottleInterval</key>
        <integer>300</integer>
</dict>
</plist>
